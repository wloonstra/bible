<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">

    <description></description>
    <display-name>BibleText</display-name>
    
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>nl.wiggertloonstra.bible.servlet.HomeServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>overview</servlet-name>
        <servlet-class>nl.wiggertloonstra.bible.servlet.OverviewServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>addText</servlet-name>
        <servlet-class>nl.wiggertloonstra.bible.servlet.AddTextServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>overview</servlet-name>
        <url-pattern>/overzicht</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>addText</servlet-name>
        <url-pattern>/tekst-toevoegen</url-pattern>
    </servlet-mapping>
    
</web-app>
